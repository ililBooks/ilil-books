{
  "family": "ililbooks-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "containerDefinitions": [
    {
      "name": "ililbooks-app",
      "image": "<Image URI>",
      "portMappings": [
        {
          "containerPort": 8081
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "docker"
        }
      ]
    }
  ]
}
